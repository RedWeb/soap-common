<?xml version="1.0" encoding="UTF-8" ?>
<container xmlns="http://symfony.com/schema/dic/services" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">


    <parameters>
        <parameter key="goetas_webservices.soap_common.metadata" type="collection"></parameter>
    </parameters>

    <services>

        <service id="goetas_webservices.soap_common.metadata_loader.dev" public="true"
                 class="GoetasWebservices\SoapServices\SoapCommon\MetadataLoader\DevMetadataLoader">
            <argument type="service" id="goetas_webservices.soap_common.metadata.generator"/>
            <argument type="service" id="goetas_webservices.wsdl2php.soap_reader"/>
            <argument type="service" id="goetas_webservices.wsdl2php.wsdl_reader"/>
        </service>

        <service id="goetas_webservices.soap_common.metadata_loader.array" public="false"
                 class="GoetasWebservices\SoapServices\SoapCommon\MetadataLoader\ArrayMetadataLoader">
            <argument>%goetas_webservices.soap_common.metadata%</argument>
        </service>

        <service id="goetas_webservices.soap_common.metadata.generator" public="false"
                 class="GoetasWebservices\SoapServices\SoapCommon\MetadataGenerator\MetadataGenerator">
            <argument id="goetas_webservices.xsd2php.naming_convention" type="service"/>
            <argument type="collection"/>
        </service>
    </services>
</container>
